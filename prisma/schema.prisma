datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Trip {
  id          Int      @id @default(autoincrement())
  fullName    String
  destination String
  startDate   DateTime
  endDate     DateTime
  budget      Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
